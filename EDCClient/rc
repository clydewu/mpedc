hostname M501
hwclock -s
mount -t proc proc /proc
mount -o remount,rw /dev/root /
mount /sys
ifconfig lo 127.0.0.1
route add -net 127.0.0.0 netmask 255.255.255.0 lo
mount /mnt/mmc
/mnt/disk/cron
/home/test/edc_net_setup.sh /home/test/edc_network.conf

if [ $? != 0 ]; then
	ifconfig eth0 192.168.7.251 netmask 255.255.255.0
	route add default gw 192.168.7.254
fi

cat /etc/motd
ln -s /home/work/gconv/ /lib/
cd /home/test/
./edc_show_ip.sh ./edc_network.conf
sleep 5
./test.out &
